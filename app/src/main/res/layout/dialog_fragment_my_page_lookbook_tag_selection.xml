<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/round_rectangle_background_type_1">
        <TextView
            android:id="@+id/my_page_user_lookbook_tag_style_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="스타일"
            android:textStyle="bold"
            style="@style/header4"
            android:layout_marginTop="16dp"
            android:layout_marginHorizontal="18dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
        <LinearLayout
            android:id="@+id/my_page_lookbook_tag_layout_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginHorizontal="@dimen/horizontal_margin"
            app:layout_constraintTop_toBottomOf="@id/my_page_user_lookbook_tag_style_text"
            app:layout_constraintStart_toStartOf="parent">
            <TextView
                android:id="@+id/my_page_lookbook_tag_casulal_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#캐주얼"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_street_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#스트릿"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_modern_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#모던"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_feminine_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#페미닌"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_dandy_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#댄디"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/my_page_lookbook_tag_layout_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginHorizontal="@dimen/horizontal_margin"
            app:layout_constraintTop_toBottomOf="@id/my_page_lookbook_tag_layout_1"
            app:layout_constraintStart_toStartOf="parent">
            <TextView
                android:id="@+id/my_page_lookbook_tag_minimal_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#미니멀"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_maximal_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#맥시멀"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_city_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#시티"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_americanCasual_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#아메카지"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_classic_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#클래식"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
        </LinearLayout>
        <TextView
            android:id="@+id/my_page_user_lookbook_tag_situation_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="상황"
            android:textStyle="bold"
            style="@style/header4"
            android:layout_marginTop="12dp"
            android:layout_marginHorizontal="18dp"
            app:layout_constraintTop_toBottomOf="@id/my_page_lookbook_tag_layout_2"
            app:layout_constraintStart_toStartOf="parent"/>
        <LinearLayout
            android:id="@+id/my_page_lookbook_tag_layout_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginHorizontal="@dimen/horizontal_margin"
            app:layout_constraintTop_toBottomOf="@id/my_page_user_lookbook_tag_situation_text"
            app:layout_constraintStart_toStartOf="parent">
            <TextView
                android:id="@+id/my_page_lookbook_tag_student_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#학생"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_office_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#직장인"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_date_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#데이트"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_blindDate_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#소개팅"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_tour_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#여행"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/my_page_lookbook_tag_layout_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginHorizontal="@dimen/horizontal_margin"
            app:layout_constraintTop_toBottomOf="@id/my_page_lookbook_tag_layout_3"
            app:layout_constraintStart_toStartOf="parent">
            <TextView
                android:id="@+id/my_page_lookbook_tag_party_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#파티"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_couple_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#커플"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_quest_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#하객"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
        </LinearLayout>
        <TextView
            android:id="@+id/my_page_user_lookbook_tag_weather_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="계절/날씨"
            android:textStyle="bold"
            style="@style/header4"
            android:layout_marginTop="12dp"
            android:layout_marginHorizontal="18dp"
            app:layout_constraintTop_toBottomOf="@id/my_page_lookbook_tag_layout_4"
            app:layout_constraintStart_toStartOf="parent"/>
        <LinearLayout
            android:id="@+id/my_page_lookbook_tag_layout_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginHorizontal="@dimen/horizontal_margin"
            app:layout_constraintTop_toBottomOf="@id/my_page_user_lookbook_tag_weather_text"
            app:layout_constraintStart_toStartOf="parent">
            <TextView
                android:id="@+id/my_page_lookbook_tag_spring_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#봄"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_summer_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#여름"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_autumn_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#가을"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_winter_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#겨울"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
            <TextView
                android:id="@+id/my_page_lookbook_tag_rain_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_round"
                android:text="#비"
                android:textColor="@color/gray_8"
                android:textStyle="bold"
                style="@style/body2"
                android:layout_marginEnd="10dp"
                android:paddingHorizontal="8dp"
                android:paddingVertical="3dp"/>
        </LinearLayout>
        <Button
            android:id="@+id/my_page_lookbook_tag_save_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="완료"
            android:textStyle="bold"
            style="@style/header4"
            android:textColor="@color/white"
            android:layout_marginTop="30dp"
            android:layout_marginHorizontal="@dimen/horizontal_margin"
            android:paddingVertical="10dp"
            app:layout_constraintTop_toBottomOf="@id/my_page_lookbook_tag_layout_5"
            app:layout_constraintStart_toStartOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>